                    <th scope="col">Email</th>
                   
                    <th scope="col">Conatct</th>
                    <th scope="col">Action</th>
                </tr>
            </thead>
            <tbody>
                {{#each rows}}
                <tr>
                    <th scope="row">{{this.Id}}</th>
                    
                    <td>{{this.Name}}</td>
                    <td>{{this.Email}}</td>
                   
                    <td>{{this.Contact}}</td>
                    <td><a href="/delete/{{this.Id}}"><i class="fa fa-trash"></i></a></td>
