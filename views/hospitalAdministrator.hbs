    <link rel="stylesheet" href="/admindash.css">
    <title>Admin</title>
    <style>
        .link {
            font-size: 98%;
            list-style-type: none;
            padding: 1.5%;
            padding-right: 20%;
            margin-top: 3.8%;
            overflow: hidden;
            left: 0px;
        }

        .items {
            float: right;
            position: relative;
            margin: 0 150px 0 55px;
            cursor: pointer;
        }

        .items::after {
            content: '';
            width: 0%;
            height: 2px;
            background: #6d350f;
            display: block;
            margin: auto;
            transition: 0.5s;
        }

        .items:hover::after {
            width: 100%;
            pointer-events: auto;
        }

        .items a {
            text-decoration: none;
            color: #6d350f;
        }

        .li-items a:active {
            color: #6d350f;

        }
    </style>
</head>

